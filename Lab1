#include <iostream>
#include <fstream>
#include <string>
#include <map>
#include <vector>

using namespace std;


vector <char> variables;
map<char, int> constructors;
string firstterm;
string secondterm;

struct Multy{
    string key;
    string num;
    struct Multy* next;
};

struct Tree {
    char value;
    int num;
    int var;
    struct Tree* left;
    struct Tree* right;

};

struct Multy* answer=NULL;


int k=0;

struct Multy* CreateMulty(struct Multy* p,int i){

    if (p==NULL){
        p=new(struct Multy);
        if (!i){
            p->key="x0";
            p->num=firstterm+','+secondterm;
            p->next=NULL;
            i++;
            p->next=CreateMulty(p->next,i);
        }
        else {
            p->key=variables[k];
            p->num="";
            p->next=NULL;
            k++;
            
        }
    }
    while (k<variables.size()){
        p->next=CreateMulty(p->next,i);
    }
    return p;
}

void Printequation(struct Multy* p){
    cout<<'{'<<p->key<<"} = {";
    if (p->num.size()) cout<<p->num;
    cout<<"}\n";
    if (p->next!=NULL) Printequation(p->next);
}

struct Tree* Term_analysis(struct Tree* p,struct Tree* q,string str,int i){
     map <char, int> ::iterator it;
    char dop_str;
    int find=0;
    while ((str[i]==')')||(str[i]==',')||(str[i]=='(')){
        i++;
    } 
    k=i;
     for (i;(str[i]!='(')&&(str[i]!=',')&&(str[i]!=')')&&(i!=str.size());i++){
        dop_str=str[i];
        
    }
    k=i;
    if (p==NULL){
        p = (struct Tree*)malloc(sizeof(struct Tree));
        p->value=dop_str;
        p->left=NULL;
        p->right=NULL;
        p->var=0;
        p->num=0;

    } 

    it = constructors.find(dop_str);
    if (it!=constructors.end()){
            if (q!=NULL) q->num--;
    p->num=constructors[dop_str];
    
    if (p->num){
        p->left= Term_analysis(p->left,p,str,k);
    } 
    if ((q!=NULL)&&(q->num))  {

        p->right=Term_analysis(p->right,q,str,k);
    }
    }
   for (int j=0;j<variables.size();j++){
        if (dop_str==variables[j]) {
            p->var=1;
            find=1;
        }
    }
    if (find){
        if (q!=NULL) {
            q->num--;
            
           if (q->num) {
            p->right=Term_analysis(p->right,q,str,k);
           }
        }    
    }
    return p;

}

 
void Printtree(struct Tree* p){
    cout<<p->value<<"\n";
    if (p->left!=NULL) Printtree(p->left); 
    if (p->right!=NULL) Printtree(p->right);
}

void Constructors(string str) {
    
    char letters;
    string numbers;
    int num = 0;
    int r = 0;

    for (int i = 0; i < str.size(); i++) {
        
        if (str[i] == '(') {
            int j=i-1;
            letters=str[j];
            r=1;
            continue;
        }
        if (str[i] == ',') continue;
        if (r){
            numbers.push_back(str[i]);
            if (str[i]==')'){
            num = stoi(numbers);
            constructors[letters] = num;
            numbers.clear();
            num = 0;
            r = 0;
            continue;
            }
        }
    
        
       
    } 
    
}

string commonpart;
int error=0;

int l=0;
int num=0;

string helper(struct Tree* term,string t){
    string dop_str;
    dop_str+=term->value;
    if (!term->var) {
        int n=constructors[term->value];
        if (n) dop_str+='(';
        t+=dop_str;
        if (term->left!=NULL){
            t=helper(term->left,t);
            t+=')';
        } 

        if (term->right!=NULL){
            t+=',';
            t=helper(term->right,t);
        } 
    }    
   if (term->var){
    t+=dop_str;
    if (term->right!=NULL){
        t+=',';
        t=helper(term->right,t);
    }
   }
   return t;
}

struct Multy* CreateNewBlock(struct Multy* p,string str){
    if (p!=NULL) p->next=CreateNewBlock(p->next,str);
    else{
        p=new(struct Multy);
        p->key=str;
        p->next=NULL;
        p->num="";
    }
    return p;
}

void Substitutions(struct Tree* term1,struct Tree* term2){
    struct Multy* dop=answer;
    string dop_str;
    string dop_str2;
    string t="";
    
   

    if ((!term1->var)&&(!term2->var)){
        if (term1->left!=NULL){
            Substitutions (term1->left,term2->left);
        } 
        if (term1->right!=NULL){
            Substitutions(term1->right,term2->right);
        } 
    }

    if ((term1->var)&&(term2->var)) {
            dop_str.clear();
            dop_str2.clear();
            dop_str+=term1->value;
        while (dop!=NULL){
            dop_str2=dop->key;
            if (dop_str2.find(dop_str)==-1) dop=dop->next;
            else{
                dop_str2.clear();
                dop_str2=term2->value;
                if (dop->key.find(dop_str2)==-1){
                dop->key+=',';
                dop->key+=term2->value;
                dop_str.clear();
                dop_str2.clear();
                }
                break;
            }
        }
        if (term1->right!=NULL) Substitutions(term1->right,term2->right);
    }
    if ((term1->var)&&(!term2->var)){
            dop_str.clear();
            dop_str2.clear();
            dop_str+=term1->value;
            while (dop!=NULL){
            dop_str2=dop->key;
            if (dop_str2.find(dop_str)==-1) dop=dop->next;
            else break;
         }
        if (dop->num.size()){
            answer=CreateNewBlock(answer,dop_str);
            while (dop->next!=NULL) dop=dop->next;
        }

        dop->num+=term2->value;
        int dt=constructors[term2->value];
        if (dt){dop->num+='(';
        struct Tree* dop_5=term2->left;
        t=helper(dop_5,t);
        dop->num+=t;
        dop->num+=')';
        }
        if ((term2->right!=NULL)&&(term1->right!=NULL)) Substitutions(term1->right,term2->right);
    }
    if ((!term1->var)&&(term2->var)){
         dop_str.clear();
        dop_str2.clear();
        dop_str+=term2->value;
         while (dop!=NULL){
            dop_str2=dop->key;
            if (dop_str2.find(dop_str)==-1) dop=dop->next;
            else break;
        }
        if (dop->num.size()){
            answer=CreateNewBlock(answer,dop_str);
            while (dop->next!=NULL) dop=dop->next;
        }

        dop->num+=term1->value;
        int dt=constructors[term1->value];
        if (dt){dop->num+='(';
        struct Tree* dop_6=term1->left;
        t=helper(dop_6,t);
        dop->num+=t;
        dop->num+=')';
        }
        dop_str2.clear();
        dop_str.clear();
 
        if ((term1->right!=NULL)&&(term2->right!=NULL)) Substitutions(term1->right,term2->right);
    }
    return;

}

string CheckTree(struct Tree* term1, struct Tree* term2,string t){
    string dop_str;

    if ((!term1->var)&&(!term2->var)){
        if (term1->value!=term2->value) {
            error=1;
            return "err";
            }
        dop_str+=term1->value;
        int dt=constructors[term1->value];

        if (dt)dop_str+='(';
        t+=dop_str;
        if (term1->left!=NULL) {
            
            t=CheckTree(term1->left,term2->left,t);
           if (dt) t+=')';
        }
        if (term1->right!=NULL){
            t+=',';
            
            t=CheckTree(term1->right,term2->right,t);
        }
    }
    if ((term1->var)&&(term2->var)){
        t+=term1->value;
        if ((term1->right!=NULL)&&(term2->right!=NULL)){
            t+=',';
            t=CheckTree(term1->right,term2->right,t);
        } 
    }
    if ((term1->var)&&(!term2->var)){
         t+=term1->value;
        if ((term1->right!=NULL)&&(term2->right!=NULL)){
            t+=',';
            t=CheckTree(term1->right,term2->right,t);
        } 
    }
    if ((!term1->var)&&(term2->var)){
         t+=term2->value;
        if ((term1->right!=NULL)&&(term2->right!=NULL)){
            t+=',';
            t=CheckTree(term1->right,term2->right,t);
        } 
    }
    return t;
}

int er2=0;
void Delete_elem(struct Multy* dop, struct Multy* prev){
    struct Multy* dop_p=answer;
    answer=prev;
    answer->next=dop->next;
    answer=dop;
    delete(answer);
    answer=dop_p;
}


void Contification(){
    struct Multy* p=answer;
    struct Multy* q=p;
    struct Multy* prev;
    struct Multy* dop_p=answer;
    char x;
    char dop;
    char y;
    int i=0;
    int begin=0;
    while (p!=NULL){
        begin=0;
        if (p->next==NULL) break;
        prev = p;
        q=p->next;
        if (p->key[i]==',') i++;
        x=p->key[i];
        if (p->num.find(x)!=-1){
            error=1;
            return;
        }
        while (q!=NULL){

           if (q->num.size()){
            for (int j=0;j<q->key.size();j++){
                if ((q->key[j])==',') continue;
                y=q->key[j];
                if (q->num.find(y)!=-1){
                    error=1;
                    return;
                }
            }
           }

            if (q->key.find(x)!=-1){
               
                if ((!q->num.size())&&(!p->num.size())){   
                    answer=p;
                   
                for (int j=0;j<q->key.size();j++){
                        if (q->key[j]==',') continue;
                        dop=q->key[j];
                        if (answer->key.find(dop)==-1){
                            answer->key+=',';
                            answer->key+=dop;
                        }
                        
                    }
                answer=dop_p;
                Delete_elem(q,prev);
                q=prev;
                
                }else{
                    if ((!p->num.size())&&(q->num.size())){

                    for (int j=0;j<p->key.size();j++){
                        if (p->key[j]==',') continue;
                        y=p->key[j];
                        if (q->num.find(y)!=-1){
                            error=1;
                            return;
                        }
                    }
                    answer=p;
                    answer->num=q->num;
                    for (int j=0;j<q->key.size();j++){
                        if (q->key[j]==',') continue;
                        dop=q->key[j];
                        if (answer->key.find(dop)==-1){
                            answer->key+=',';
                            answer->key+=dop;
                        }
                    }
                    answer=dop_p;
                    Delete_elem(q,prev);
                    q=prev;
                }
                else {
                     if ((p->num.size())&&(!q->num.size())){
                        
                        for (int j=0;j<q->key.size();j++){
                        if (q->key[j]==',') continue;
                        y=q->key[j];
                        if (p->num.find(y)!=-1){
                            error=1;
                            return;
                        }
                        }
                    answer=p;
                    
                    for (int j=0;j<q->key.size();j++){

                        if (q->key[j]==',') continue;
                        
                        dop=q->key[j];
                       
                        if (p->key.find(dop)==-1){
                            answer->key+=',';
                            answer->key+=dop;
                        }
                    }
                    answer=dop_p;
                    Delete_elem(q,prev);
                    q=prev;
                    

                }
                else {
                    if ((p->num.size())&&(q->num.size())){

                    for (int j=0;j<q->key.size();j++){
                        if (q->key[j]==',') continue;
                        y=q->key[j];
                        if (p->num.find(y)!=-1){
                            error=1;
                            return;
                        }
                        }
                    for (int j=0;j<p->key.size();j++){
                        if (p->key[j]==',') continue;
                        y=p->key[j];
                        if (q->num.find(y)!=-1){
                            error=1;
                            return;
                        }
                    }
                    k=0;
                    begin++;
                    struct Tree* dop_term1=NULL;
                
                    dop_term1=Term_analysis(dop_term1,NULL,p->num,0);
               
                    struct Tree* dop_term2=NULL;
                    k=0;
                    
                    dop_term2=Term_analysis(dop_term2,NULL,q->num,0);
                 
                    l=0;
                    num=0;
                    commonpart.clear();
                    string dop_str15;
                    dop_str15=CheckTree(dop_term1,dop_term2,dop_str15);
                    answer=p;
                    answer->num.clear();
                    
                    answer->num+=dop_str15;
                    answer=dop_p;
                    Delete_elem(q,prev);
                    Substitutions(dop_term1,dop_term2);
                    if (error) return;
                    q=answer;
                    prev=answer;
                    p=answer;
                    break;
                }
                }
                }
                }
            }
            else{
                if ((p->num.size())&&(q->num.size())){
                     if ((p->num)==(q->num)){
                    answer=p;
                    for (int j=0;j<q->key.size();j++){
                        if (q->key[j]==',') continue;
                        y=q->key[j];
                        if (answer->key.find(y)==-1){
                            answer->key+=',';
                            answer->key+=y;
                        }
                    }

                    answer=dop_p;
                    Delete_elem(q,prev);
                    q=prev;
                }
                }
               
            }
            prev=q;
            q=q->next;
        }
   if ((q==answer)&&(p==answer)&&(prev==answer)) i=0;
   else {
    i++;
    if (i==p->key.size()){
        p=p->next;
        i=0;
    }
   }
        
}
}

int main(){
    int find_constructors;
    int find_variables;
    int find_firstterm;
    int find_secondterm;
    string str;
    ifstream in("test9.txt");
    if (!in) {
      
        return -1;
    }
    while (in) {
        in >> str;
        if (str=="constructors") {
            in >> str;
            if (str!="=") return -1;
            find_constructors = 1;
            while (true) {
                
                in >>str;
                if (str == "variables") break;
                Constructors(str);
                
            }
            
        }
    
        
        if (str == "variables") {
            in >> str;
            if (str != "=") return -1;
            in >> str;
            find_variables = 1;
  


            int last_elem = 0;
            while (true) {
                if (str == "First") break;

                for (int i=0; i < str.size(); i++) {
                    if (str[i] != ',') {
                        variables.push_back(str[i]);
                        last_elem=1;
                    }
                }
                
                if (!last_elem) variables.push_back(str[0]);
                in >> str;
                
                last_elem = 0;
            }
        }
        
        if (str == "First") {
            in >> str;
            in >> str;
            firstterm = str;
        }
        in >> str;
        if (str == "Second") {
            in >> str;
            in >> str;
            secondterm = str;
            
        }
        
    }
    
    
    answer=CreateMulty(answer,0);
    k=0;
    struct Multy* p=answer;
    answer=answer->next;
    delete(p);

    Tree* term1=NULL;
    
    term1=Term_analysis(term1,NULL,firstterm,0);
    
    Tree* term2=NULL;
    k=0;
    term2=Term_analysis(term2,NULL,secondterm,0);
    k=0;
    commonpart=CheckTree(term1,term2,commonpart);
   l=0;
   string commonpart_base=commonpart;
    if (error==0) cout<<commonpart_base<<"\n"; 
    else cout<<"error1";
    Substitutions(term1,term2);
    //Printequation(answer);
    //Substitutions(term1,term2);
   Contification();
   cout<<"\n";
    if ((!er2)&&(!error)) Printequation(answer);
    if ((er2)||(error)) cout<<"error\n";
   
}
